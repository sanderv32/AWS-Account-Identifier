{
   "action": {
      "default_popup": "popup.html",
      "default_title": "Options"
   },
   "background": {
      "scripts": ["background.js"]
   },
   "content_scripts": [ {
      "js": [ "main.js" ],
      "matches": [ "*://*.console.aws.amazon.com/*" ],
      "all_frames": true
   }, {
      "js": [ "main-sso.js" ],
      "matches": [ "*://*.awsapps.com/start#/", "*://*.awsapps.com/start/*" ]
   } ],
   "description": "Adds a description with a specific color to the AWS's navigation bar depending on the account you are connected to.",
   "host_permissions": [ "*://*.console.aws.amazon.com/*" ],
   "icons": {
      "128": "icon.png"
   },
   "manifest_version": 3,
   "name": "AWS Account Identifier",
   "permissions": [ "storage" ],
   "update_url": "https://edge.microsoft.com/extensionwebstorebase/v1/crx",
   "version": "1.0.2"
}
